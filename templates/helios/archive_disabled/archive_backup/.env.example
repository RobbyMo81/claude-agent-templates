# Helios Environment Variables
# Copy this file to .env.local and replace PLACEHOLDER_API_KEY with your actual Gemini API key

# Gemini API Configuration
VITE_GEMINI_API_KEY=PLACEHOLDER_API_KEY
GEMINI_API_KEY=PLACEHOLDER_API_KEY

# API Configuration
VITE_API_HOST=localhost
VITE_API_PORT=8080
VITE_API_PROTOCOL=http

# Development Configuration
VITE_FRONTEND_PORT=5173

# Note: The VITE_ prefix makes these variables available to the frontend build
# The non-prefixed versions are for backend use
